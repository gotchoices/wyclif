    "cert": "bin/makecert",
    "init": "bin/localcerts",
    "profile": "webpack --profile --json >/tmp/webpack-stats.json",
    "preversion": "npm-run-all build && git add -A",
    "certificate": "bin/makecert"
  },
  "bin": {
    "wyclif": "bin/server.js",
    "wyclif-info": "bin/info.js",
    "wyclif-cert": "bin/makecert"
