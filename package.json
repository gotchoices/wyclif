{
  "name": "wyclif",
  "version": "1.2.0",
  "description": "WyattERP Control Layer InterFace",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-hot": "webpack-dev-server --hot --config build/webpack.test.conf.js",
    "dev-build": "webpack --mode development --watch --progress --config build/webpack.dist.conf.js --stats-error-details",
    "build": "webpack --mode production --config build/webpack.dist.conf.js",
    "dev-server": "bin/server.js --spaPort=0",
    "dev": "npm-run-all --parallel dev-build dev-server",
    "server": "bin/server.js",
    "build-schema": "cd schema && wyseman objects text defs init -S ./schema.json",
    "start": "bin/localcerts -b web && bin/server.js",
    "cert": "bin/makecert",
    "initcerts": "bin/localcerts web",
    "profile": "webpack --profile --json >/tmp/webpack-stats.json",
    "preversion": "npm-run-all build && git add -A"
  },
  "bin": {
    "wyclif": "bin/server.js",
    "wyclif-info": "bin/info.js",
    "wyclif-cert": "bin/makecert"
  },
  "keywords": [],
  "author": "Kyle Bateman",
  "license": "MIT",
  "homepage": null,
  "dependencies": {
    "express": "^4.17.1",
    "serve-favicon": "^2.5.0",
    "string-hash": "^1.1.3",
    "winston": "^3.8.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@vue/compiler-sfc": "3.4.19",
    "d3": "^7.4.2",
    "babel-loader": "^8.2.3",
    "core-js": "^3.8.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "less": "^3.13.1",
    "less-loader": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "vue": "^3.4.19",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
